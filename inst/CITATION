year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "manual",
         title = "eeiR: Ecological Evaluation Index in R",
         author = personList(as.person("Izidro Sousa-Filho"),
           as.person("Aline Queiroz"),
           as.person("Sotiris Orfanidis" )),
         year = year,
         note = note,
		 url = "https://github.com/salah31416/eeiR")
